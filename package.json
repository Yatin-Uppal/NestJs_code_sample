{
  "name": "osqo",
  "version": "0.0.1",
  "description": "It has all APIs for OCQO project",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npx nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:local": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "sls:offline-local": "./sls_start.sh offline local",
    "sls:offline-development": "./sls_start.sh offline development",
    "sls:offline-production": "./sls_start.sh offline production",
    "sls:offline-uat": "./sls_start.sh offline uat",
    "sls:offline-test": "./sls_start.sh offline test",
    "sls:deploy": "./sls_start.sh",
    "sls:deploy-pipeline": "./sls_start.sh pipeline",
    "sls:remove": "./sls_remove.sh",
    "sls:remove-old-versions-development": "npx sls prune -n 5 --stage development",
    "sls:remove-old-versions-production": "npx sls prune -n 5 --stage production",
    "sls:remove-old-versions-test": "npx sls prune -n 5 --stage test",
    "sls:remove-old-versions-uat": "npx sls prune -n 5 --stage uat",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "compodoc": "compodoc",
    "generate-docs": "compodoc -p ./tsconfig.doc.json",
    "serve-docs": "compodoc -s ./tsconfig.doc.json"
  },
  "overrides": {
    "pdf-generator-api": {
      "axios": "^1.3.4"
    },
    "@adobe/pdfservices-node-sdk": {
      "jsonwebtoken": "9.0.0"
    },
    "box-node-sdk": {
      "jsonwebtoken": "9.0.0"
    },
    "serverless-offline": {
      "jsonwebtoken": "9.0.0"
    }
  },
  "dependencies": {
    "@adobe/pdfservices-node-sdk": "^2.2.2",
