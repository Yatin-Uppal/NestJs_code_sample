    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.12",
    "@nestjs/swagger": "^6.2.1",
    "@osqo-npm/constants-npm-package": "^1.0.12",
    "@osqo-npm/osqo-businesrules-package": "^1.0.6",
    "@sendgrid/client": "^7.7.0",
    "@sendgrid/mail": "^7.6.2",
    "@serverless/compose": "^1.3.0",
    "@vendia/serverless-express": "^4.8.2",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1070.0",
    "aws-serverless-express": "^3.4.0",
    "axios": "^1.3.4",
    "base64-stream": "^1.0.0",
    "bcrypt": "^5.0.1",
    "box-node-sdk": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto": "^1.0.1",
    "csvtojson": "^2.0.10",
    "express": "^4.17.3",
    "intercom-client": "^3.1.5",
    "ioredis": "^5.1.0",
    "jwks-rsa": "^2.1.4",
    "mailcomposer": "^4.0.2",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "mongoose": "^6.2.6",
    "mssql": "^8.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pdf-generator-api": "^1.1.0",
    "redis": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sls": "^0.1.0",
    "webpack": "^5.76.3",
    "winston": "^3.8.0",
    "winston-loggly-bulk": "^3.2.1",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^9.3.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.12",
    "@types/aws-lambda": "^8.10.92",
    "@types/base64-stream": "^1.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "npm-force-resolutions": "0.0.10",
    "prettier": "^2.3.2",
    "serverless-certificate-creator": "^1.6.0",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-domain-manager": "^6.1.0",
    "serverless-dotenv-plugin": "^4.0.1",
    "serverless-offline": "^8.8.0",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "serverless-plugin-typescript": "^2.1.2",
    "serverless-prune-plugin": "^2.0.1",
    "serverless-stack-termination-protection": "^2.0.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
